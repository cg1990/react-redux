{
    "ecmaFeatures":{
        "jsx": true,
        "modules": true
    },
    "env":{
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules":{
        "quotes":[2, "single"],
        "strict":[2, "never"],
        "react/jsx-user-react":2,
        "react/jsx-user-vars": 2,
        "react/react-in-jsx-scope":2
    },
    "plugins":[
        "react"
    ]
}